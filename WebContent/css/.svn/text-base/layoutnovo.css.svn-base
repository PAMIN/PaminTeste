#navMenu{
	margin:0;
	padding:0;
}
#navMenu ul{
	margin:0;
	padding:0;
	line-height:24px;
	z-index:2;
}
#navMenu li{
	margin:0;
	padding:0;
	list-style:none;
	float:left;
	position: relative;
	background:#162d56;
}
#navMenu ul ul li{
	background:#5b79b5;
}
#navMenu ul li a{
	text-align: center;
	font-family:"Comic Sans MS", cursive;
	text-decoration:none;
	height:24px;
	width:152px;
	display:block;
	color:#FFF;	
}
#navMenu ul ul{
	position:absolute;
	visibility: hidden;
	top:24px;
}

#navMenu ul li:hover ul{
	visibility: visible;
}
#navMenu li:hover{
	background:#09F;
}
#navMenu ul li:hover ul li a:hover{
	background:#AFC6F9;
	color:#000;
}
.clearFloat{
	clear:both;
	margin:0px;
	padding:0px;
}
.close{display:block; text-align:right; margin-top: 100px; margin-right: 50px;}